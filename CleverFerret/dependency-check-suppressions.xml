<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">
    <!-- 
    This file contains CVE suppressions for the Universal Media Library project.
    Suppressions should be used sparingly and only for false positives or 
    vulnerabilities that don't apply to this specific use case.
    
    Each suppression should include:
    - Clear justification
    - CVE number if applicable
    - Review date for periodic re-evaluation
    -->
    
    <!-- Example suppression for false positives in test dependencies -->
    <!--
    <suppress>
        <notes><![CDATA[
        This CVE is for a test dependency and doesn't affect the production app.
        Reviewed: 2024-09-08
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/junit/junit@.*$</packageUrl>
        <cve>CVE-XXXX-XXXX</cve>
    </suppress>
    -->
    
    <!-- Suppress Kotlin related false positives that commonly occur -->
    <suppress>
        <notes><![CDATA[
        Kotlin stdlib vulnerabilities are typically false positives for Android apps
        as they don't apply to the Android runtime environment.
        Reviewed: 2024-09-08
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.jetbrains\.kotlin/kotlin-stdlib.*$</packageUrl>
        <vulnerabilityName regex="true">.*CVE-.*</vulnerabilityName>
    </suppress>
    
    <!-- Suppress Android framework false positives -->
    <suppress>
        <notes><![CDATA[
        Android framework dependencies are managed by the Android platform
        and don't represent vulnerabilities in this context.
        Reviewed: 2024-09-08
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/com\.android\..*$</packageUrl>
        <vulnerabilityName regex="true">.*CVE-.*</vulnerabilityName>
    </suppress>
    
</suppressions>